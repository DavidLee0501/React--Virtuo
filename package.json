{
  "name": "react-virtuoso",
  "author": "Petyo Ivanov",
  "sideEffects": false,
  "version": "0.0.0-development",
  "homepage": "https://virtuoso.dev/",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/react-virtuoso.esm.js",
  "exports": {
    ".": {
      "require": "./dist/react-virtuoso.cjs.production.min.js",
      "import": "./dist/react-virtuoso.esm.js"
    }
  },
  "keywords": [
    "react",
    "reactjs",
    "virtual",
    "endless",
    "load more",
    "list",
    "scrolling",
    "infinite",
    "virtualized",
    "grid",
    "feed",
    "chat"
  ],
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "e2e": "tsdx test --run-in-band --passWithNoTests --config jest.e2e.config.js",
    "lint": "tsdx lint",
    "typecheck": "tsc --noEmit",
    "prepare": "tsdx build",
    "preview": "node e2e/preview.js",
    "browse-examples": "node e2e/test-cases.mjs && parcel e2e/__examples.html",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@virtuoso.dev/react-urx": "^0.2.3",
    "@virtuoso.dev/urx": "^0.2.3",
    "react-app-polyfill": "^1.0.6",
    "resize-observer-polyfill": "^1.5.1"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "devDependencies": {
    "@emotion/core": "^10.1.0",
    "@emotion/styled": "^10.0.27",
    "@types/faker": "^5.1.4",
    "@types/jsdom": "^16.2.3",
    "@types/lodash": "^4.14.165",
    "@types/parcel-bundler": "^1.12.1",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^16.9.3",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-json": "^2.1.1",
    "faker": "^5.1.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jest-playwright-preset": "^1.4.4",
    "jsdom": "^16.4.0",
    "lodash": "^4.17.20",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "playwright": "^1.8.0",
    "playwright-chromium": "^1.8.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^16.13.1",
    "semantic-release": "^17.3.0",
    "ts-jest": "^26.4.4",
    "tsdx": "^0.13.2",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "resolutions": {
    "@types/react": "^16.14.2"
  },
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 140,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/petyosi/react-virtuoso.git"
  }
}
